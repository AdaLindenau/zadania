select distinct nazwa from Dostawa, Towar where Towar.id = Dostawa.towar and Dostawa.ilosc = 122;
select distinct nazwisko from Klienci, Sprzedaz, Towar where Klienci.id = Sprzedaz.id_klient and Sprzedaz.id_towar = Towar.id;
select firma, concat(imie, ' ', nazwisko, ', ', adres, ', ', miasto) as kontakt from Klienci;
select nazwa, avg(ilosc) from Sprzedaz, Towar where Sprzedaz.id_towar = Towar.id group by nazwa;
select nazwa, count(*) from Sprzedaz, Towar where Sprzedaz.id_Towar = Towar.id and nazwa = "Papier ksero";
select Klienci.firma, Towar.nazwa, Sprzedaz.ilosc from Sprzedaz, Klienci, Towar where Sprzedaz.ilosc = max(Sprzedaz.ilosc) and Sprzedaz.id_Klient = Klienci.id and Sprzedaz.id_Towar = Towar.id; * FIX
